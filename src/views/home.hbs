<h1>home</h1>
<div>
  {{#each products}}
    <div class="div">
      <div class="img">
        <img src="{{this.thumbnail}}" alt="{{this.title}}" />
      </div>
      <div class="product">
        <h2>{{this.title}}</h2>
        <h3>{{this.description}}</h3>
        <h4>$ {{this.price}}</h4>
        <h5>stock: {{this.stock}}</h5>
      </div>
    </div>

  {{/each}}
</div>